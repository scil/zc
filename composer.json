{
  "_comment": "ext-bcmath is  required by moontoast/matt with is required by laravel/telescope",
  "name": "zc/zc",
  "description": "using LaravelFly, nodejs, thrift, pjax(layout $IS_PJAX); js dependent_func rely on LAST-SCRIPT",
  "keywords": [
    "zhenc",
    "真城"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "ext-mbstring": "*",
    "apache/thrift": "^0.11.0",
    "astrotomic/laravel-translatable": "^11.1",
    "encore/laravel-admin": "^1.7",
    "fideloper/proxy": "~4.0",
    "laravel/framework": "^6.6",
    "laravel/tinker": "^1.0",
    "mcamara/laravel-localization": "^1.3",
    "predis/predis": "^1.1",
    "scil/laravel-fly": "dev-master"
  },
  "require-dev": {
    "ext-bcmath": "*",
    "barryvdh/laravel-debugbar": "^3.1",
    "barryvdh/laravel-ide-helper": "dev-master",
    "beyondcode/laravel-dump-server": "^1.0",
    "eaglewu/swoole-ide-helper": "dev-master",
    "facade/ignition": "^1.4",
    "facade/ignition-code-editor": "^1.0",
    "facade/ignition-self-diagnosis": "^1.0",
    "facade/ignition-tinker-tab": "^1.0",
    "spatie/laravel-web-tinker": "1.5.1",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "laravel/telescope": "^2.1",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0"
  },
  "config": {
        "optimize-autoloader": true,
    "preferred-install": "dist",
        "sort-packages": true
  },
  "extra": {
    "laravel": {
            "dont-discover": []
    }
  },

  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
    
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "LaravelFly\\Tests\\": "vendor/scil/laravel-fly/tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
  "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
  
    "post-update-cmd": [
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta",
      "node resources/bin/hack.js -c full"
    ]
  },

  "repositories": [
    {
      "type": "path",
      "url": "vendor/scil/laravel-fly-local"
    },
    {
      "type": "path",
      "url": "vendor/scil/laravel-fly-files-local"
    }
  ]
  
}
